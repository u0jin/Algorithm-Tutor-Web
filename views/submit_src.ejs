<body> 

<header class="header clearfix">
    <a class="navbar-brand font-weight-bold" href="/" >  home </a>
</header>


<main role="main">
    <div class="container mt-3">
        
        <form action="submit_src" method="get">

            <div class="text-center">
            Problem <span class=blue><b>1</b></span>
            
                <div class="col-12 col-sm-12 mx-auto" id="result">
                    <pre>
                        
                        
                        








                        <h1> 실행결과 = <%= result %></h1>
                    </pre>

                    <div style="height:200px" id="source">
                    </div>
                    <a class="navbar-brand font-weight-bold" href="/problem" > 문제 보기  </a> &ensp; &emsp; &emsp;
                    <a class="navbar-brand font-weight-bold" href="/submit" > 코드 보기 </a>
                </br> </br>
            </br>


                </div>

            <br>


            </form>
    </div>
</main>

<script>
	document.getElementById("source").style.fontSize='1.0rem';
    ace.require("ace/ext/language_tools");
	var editor = ace.edit("source");
	var theme = 'chrome';
	editor.setTheme('ace/theme/'+theme);
	switchLang(1);
	editor.setOptions({
		    enableBasicAutocompletion: true,
		    enableSnippets: true,
		    enableLiveAutocompletion: true,
		    maxLines: 100,
		    minLines: 15,
		    autoScrollEditorIntoView: true
	});
	reloadtemplate($("#language").val()); 
    </script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js" integrity="sha384-vFJXuSJphROIrBnz7yo7oB41mKfc8JzQZiCq4NCceLEaO4IHwicKwpJf9c9IpFgh" crossorigin="anonymous"></script>
